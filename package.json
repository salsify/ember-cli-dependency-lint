{
  "name": "ember-cli-dependency-lint",
  "version": "2.0.1",
  "description": "Lint your app's addon dependencies, making sure you only have one version of each.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "salsify/ember-cli-dependency-lint",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember server",
    "test": "ember test",
    "test:all": "ember try:each",
    "test-browser": "ember test",
    "test-node": "mocha tests-node/{unit,acceptance}/**/*-test.js"
  },
  "dependencies": {
    "archy": "^1.0.0",
    "broccoli-plugin": "^1.3.0",
    "chalk": "^2.3.0",
    "semver": "^5.5.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^0.7.0",
    "chai": "^4.2.0",
    "console-ui": "^2.2.2",
    "ember-ajax": "^3.1.0",
    "ember-cli": "~3.4.3",
    "ember-cli-addon-tests": "^0.11.0",
    "ember-cli-babel": "^7.1.2",
    "ember-cli-dependency-checker": "^3.0.0",
    "ember-cli-htmlbars": "^3.0.0",
    "ember-cli-htmlbars-inline-precompile": "^1.0.3",
    "ember-cli-inject-live-reload": "^1.8.2",
    "ember-cli-qunit": "^4.3.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0-beta.1",
    "ember-cli-test-loader": "^2.2.0",
    "ember-cli-uglify": "^2.1.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.1.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-resolver": "^5.0.1",
    "ember-source": "~3.4.0",
    "eslint": "^5.6.1",
    "eslint-plugin-node": "^7.0.1",
    "loader.js": "^4.7.0",
    "mocha": "^5.2.0",
    "qunit-dom": "^0.8.0",
    "strip-indent": "^2.0.0"
  },
  "engines": {
    "node": "10.* || >= 12.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
